{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "globals": {
        "process": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaVersion": 9,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "react/prop-types": 0,
        "react/jsx-uses-vars": [2],
        "react/jsx-no-undef": "error",
        "no-console": 0
    }
}
